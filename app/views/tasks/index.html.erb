<h1>Tasks</h1>

<div>
  <% @users.each do |user| %>
    <p>
      <%= link_to user.email, user %>
    </p>
  <% end %>
</div>

<div>
  <% @tasks.each do |task| %>
    <%= render task %>
    <p>
      <%= link_to task.title, task %>
    </p>
  <% end %>
</div>


<%= link_to "New task", new_task_path %>
